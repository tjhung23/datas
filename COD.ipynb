    "            return None\n",
    "    else: \n",
    "        return tokens[0]\n",
    "def get_loc_id_kho_buucuc8(x):\n",
    "    import re\n",
    "    tokens = [number for number in re.findall(\"\\d{8}[_]BUU\", str(x)) ]\n",
